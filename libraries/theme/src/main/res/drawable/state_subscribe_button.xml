<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:state_pressed="true"
		android:state_checked="false"
		android:drawable="@drawable/ic_focused_subscribe"/>
	<item
		android:state_checked="false"
		android:drawable="@drawable/ic_default_subscribe"/>
	<item
		android:state_pressed="true"
		android:state_checked="true"
		android:drawable="@drawable/ic_selected_focused_subscribe"/>
	<item
		android:state_checked="true"
		android:drawable="@drawable/ic_selected_subscribe"/>
</selector>